#nprogress
  display none
  pointer-events none
  position relative
  .bar
    background black
    absolute top 0 left 0
    height 2px
    width 100%
    z-index 1031
  .peg
    absolute right 0
    box-shadow 0 0 10px #29d, 0 0 5px #29d
    display block
    height 100%
    opacity 1
    transform rotate(3deg) translate(0, -4px)
    width 100px
